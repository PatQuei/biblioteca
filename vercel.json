{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".next",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": [
    "gru1"
  ],
  "env": {
    "NODE_ENV": "production",
    "DATABASE_URL": "file:/tmp/dev.db",
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "DATABASE_URL": "file:/tmp/dev.db",
      "PRISMA_GENERATE_SKIP_DOWNLOAD": "true"
    }
  }
}